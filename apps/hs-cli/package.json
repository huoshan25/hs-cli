{
  "name": "create-hs-cli",
  "version": "0.0.27",
  "private": false,
  "description": "火山 CLI node脚手架工具",
  "main": "dist/index.js",
  "bin": {
    "create-hs-cli": "bin/hs-cli.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "copy-templates": "node -e \"require('fs-extra').copySync('src/templates', 'dist/templates')\"",
    "prepublishOnly": "npm run build && npm run copy-templates",
    "start": "node bin/hs-cli.js",
    "dev": "ts-node src/index.ts",
    "test": "echo \"No tests yet\"",
    "lint": "eslint src --ext .ts",
    "cli:create": "node bin/hs-cli.js create",
    "cli:generate": "node bin/hs-cli.js generate",
    "cli:init": "node bin/hs-cli.js init"
  },
  "dependencies": {
    "@huo-shan/utils": "workspace:*",
    "commander": "^10.0.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.5",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.5",
    "@types/node": "^18.15.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "cli",
    "scaffold",
    "generator",
    "create",
    "vue",
    "nuxt",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huoshan25/node-cli-test"
  },
  "publishConfig": {
    "access": "public",
    "dependencies": {
      "@huo-shan/utils": "^0.0.1"
    }
  }
}
